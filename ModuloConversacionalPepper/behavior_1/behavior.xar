            id = self.navigation_service.navigateToInMap([-0.8541533350944519, 0.07912316173315048, 0.0], _async=True)
            self.respuesta(id)
            contador += 1
        time.sleep(0.5)

        if id == 0:
            guess = [-0.8541533350944519, 0.07912316173315048]
